<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= htmlWebpackPlugin.options.title %></title>
  </head>
  <body>
    <div class="container">
      <div class="github-container">
        <a href="https://github.com/stefanbrkic1" class="github-link" target="_blank">@stefanbrkic1</a>
        <div class="github"></div>
      </div>
      <div class="main">
        <div class="landing-text">THE CLASSIC NAVAL COMBAT GAME</div>
        <div class="battleship-text">BATTLESHIP</div>
        <button type="button" class="play-btn" id="playBtn">PLAY</button>
      </div>
      <div class="switch-container">
        <div class="audio-icon"></div>
        <label class="switch" id="audioSwitch">
          <input type="checkbox">
          <span class="slider"></span>
        </label>
      </div>

    </div>
  </body>
</html>
